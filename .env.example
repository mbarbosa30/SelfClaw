# Required
DATABASE_URL=postgresql://user:pass@host:5432/selfclaw
SESSION_SECRET=your-session-secret-here
ADMIN_PASSWORD=your-admin-password

# Celo wallet private key for gas subsidies and sponsored liquidity
CELO_PRIVATE_KEY=your-celo-private-key

# Optional: OpenAI API key (miniclaw chat, memory, feed digest)
OPENAI_API_KEY=your-openai-api-key

# Optional: Hostinger API (DNS management)
HOSTINGER_API_TOKEN=your-hostinger-api-token

# Optional: Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
