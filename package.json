    "@types/node": "^25.1.0",
    "@types/passport": "^1.0.17",
    "@types/pg": "^8.16.0",
    "connect-pg-simple": "^10.0.0",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "express": "^4.22.1",
    "express-session": "^1.19.0",
    "memoizee": "^0.4.17",
    "openai": "^6.17.0",
